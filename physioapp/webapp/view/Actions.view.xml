<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="sapcp.cf.strongpeople.physioapp.controller.Actions" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Actions" showNavButton="true" navButtonPress="goBack">
				<headerContent>
					<Button icon="sap-icon://add-employee" tooltip="Add a patient" press="createPatientPress"/>
				</headerContent>
				<content>
					<List>
					  <StandardListItem type="Active" press="patientsPress" title="Patients"/>
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>